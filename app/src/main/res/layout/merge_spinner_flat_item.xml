<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

        <com.easy.make.tenantmaker.base.component.text.TextView
            android:id="@+id/flat_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:maxLines="1"
            android:paddingBottom="@dimen/dimen_2"
            android:textColor="@color/grey_charcoal"
            android:textSize="@dimen/font_14"
            app:fontTypeface="museosans500"
            tools:text="Arnold Randy" />

        <com.easy.make.tenantmaker.base.component.text.TextView
            android:id="@+id/flat_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:maxLines="1"
            android:layout_below="@+id/flat_name"
            android:textColor="@color/grey_cool"
            android:textSize="@dimen/font_12"
            app:fontTypeface="museosans500"
            tools:text="Shri Sai Paying Guest" />



</merge>